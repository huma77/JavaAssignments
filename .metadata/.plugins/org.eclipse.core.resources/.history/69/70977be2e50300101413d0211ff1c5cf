package com.assign.test2;

public class Vehicle {
	String brand;
	double speed;
	/*Vehicle(String brand, double speed){
		this.brand = brand;
		this.speed = speed;
	}*/
	public Vehicle(String brand, double speed) {
		super();
		this.brand = brand;
		this.speed = speed;
	}
	void displayDetails() {
		System.out.println("Brand: "+brand);
		System.out.println("Speed: "+speed);
	}
}
