package com.basics.quest1;

public class BookMain {

	public static void main(String[] args) {
		Book book = new Book("Mr ", "Jimmy ", 400, " A-Class");
		book.getDetails();
		String result = book.checkBookType();
		System.out.println(result);
		
		Book book1 = new Book("Ms ","Anchal ", 900, " C-Class");
		book1.getDetails();
		System.out.println();
	}

}
