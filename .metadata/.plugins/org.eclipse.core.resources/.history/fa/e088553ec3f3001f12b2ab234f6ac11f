package com.demo.training;

public class SwitchDemo {

	public static void main(String[] args) {
		int x =0, y=0;
		switch (x+y) {
			case 30: 
				System.out.println((x+y));
				break;
			case 50:
				System.out.println((x+y));
				break;
			case 80:
				System.out.println((x+y));
				break;
		}
			
		
		}
	}


